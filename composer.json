{
    "require": {
        "php":                                 ">=5.4.0",
        "silex/silex":                         "1.0.*",
        "twig/twig":                           "1.*",
        "symfony/http-foundation":             "2.2.*",
        "symfony/http-kernel":                 "2.2.*",
        "symfony/twig-bridge":                 "2.2.*",
        "jtreminio/vagrant-puppet-lamp":       "1.*@dev",
        "jfryman/puppet-nginx":                "dev-master",
        "michaelperrin/puppetlabs-postgresql": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit":                           "3.8.*@dev",
        "whatthejeff/nyancat-phpunit-resultprinter": "1.0.*@dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "Puphpet": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/frastel/puppet-nginx"
        },
        {
            "type": "vcs",
            "url": "https://github.com/michaelperrin/puppetlabs-postgresql"
        }
    ]
}
