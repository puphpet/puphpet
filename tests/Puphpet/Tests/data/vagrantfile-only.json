{
    "vagrantfile": {
        "vm": {
            "box": "wheezy64",
            "box_url": "http://puppet-vagrant-boxes.puppetlabs.com/debian-70rc1-x64-vbox4210.box",
            "graceful_halt_retry_count": "3",
            "graceful_halt_retry_interval": "1",
            "guest": ":linux",
            "hostname": null,
            "network": [
                ":private_network, ip: '192.168.56.101'",
                ":forwarded_port, guest: 8002, host: 8002"
            ],
            "provider": {
                "virtualbox": [
                    "customize ['modifyvm', :id, '--natdnshostresolver1', 'on']",
                    "customize ['modifyvm', :id, '--memory', 512]"
                ]
            },
            "provision": {
                "puppet": {
                    "manifests_path": "manifests",
                    "module_path": "modules",
                    "options": [
                        "['--verbose']",
                        "--hiera_config /vagrant/hiera.yaml"
                    ]
                },
                ":shell": ":inline => 'if [[ ! -f /apt-get-run ]]; then sudo apt-get update && touch /apt-get-run; fi'"
            },
            "synced_folder": "'./', '/var/www', id: 'vagrant-root' , :nfs => false",
            "usable_port_range": "2200..2250"
        },
        "ssh": {
            "host": null,
            "port": null,
            "private_key_path": null,
            "username": "vagrant",
            "guest_port": null,
            "keep_alive": "true",
            "max_tries": "100",
            "timeout": "30",
            "forward_agent": "false",
            "forward_x11": "false",
            "shell": "bash -l"
        },
        "vagrant": {
            "host": ":detect"
        }
    }
}
